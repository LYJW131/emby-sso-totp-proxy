# 示例 TOTP 密钥配置，支持多个用户，16位Base32密钥
# Example TOTP secret config, supports multiple users, 16-character Base32 secrets
USER_TOTP_SECRETS = '{"yourusername": "YOURTOTPSECRET"}'  

# 示例 Emby API 密钥（请替换为实际 API Key）
# Example Emby API Key (replace with your real key)
EMBY_API_KEY = 1234567890abcdef1234567890abcdef

# 示例 Emby 后端地址
# Example Emby backend address
EMBY_BACKEND = http://127.0.0.1:8096

# 示例服务器域名
# Example server name
SERVER_NAME = example.com

# SSL 证书与私钥路径示例
# Example SSL certificate and private key path
SSL_CERT_PATH = /path/to/your/fullchain.pem
SSL_KEY_PATH = /path/to/your/privkey.pem

# 跳过提供商按钮
# Skip provider button
OAUTH2_PROXY_SKIP_PROVIDER_BUTTON = true

# 请前往 https://oauth2-proxy.github.io/oauth2-proxy/ 来配置其他必须的 OAUTH2_PROXY 环境变量
# Please visit https://oauth2-proxy.github.io/oauth2-proxy/ to configure other required OAUTH2_PROXY environment variables